[user]
  name = Patrick Turley [pturley]
  email = patrick.turley@gmail.com
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = auto
[diff]
  renames = true
[alias]
  changes=diff --name-status -r
  st=status
  co=checkout
  ci=commit -a
  undo=checkout -f
[core]
  excludesfile = ~/.gitignore
[mirror]
  summary = true
[branch]
  autosetupmerge = true
[help]
  autocorrect = 1
[push]
  default = matching
